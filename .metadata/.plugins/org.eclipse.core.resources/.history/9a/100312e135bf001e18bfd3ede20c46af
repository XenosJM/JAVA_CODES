package edu.java.variable02;

public class IntegerMain {

	public static void main(String[] args) {
		System.out.println("정수(Integer) 자료형");
		
		byte n1 = 5;
		System.out.println("n1 = " + n1);
		n1 = Byte.MAX_VALUE;
		System.out.println("n1 = " + n1);
		
		
	} // end main()
	
} // end IntegerMain
