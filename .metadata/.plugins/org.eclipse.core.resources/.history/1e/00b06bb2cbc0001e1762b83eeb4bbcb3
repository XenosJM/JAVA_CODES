package edu.java.while02;

public class WhileMain02 {

	public static void main(String[] args) {
//		2 * 1 = 2
//		2 * 2 = 4
//		2 * 3 = 6
//		...
//		2 * 9 = 18
		System.out.println("구구단 연습");
		
		int i = 1;
		while (i <= 9) {
			System.out.println(2 + " * " + i + " = " + (2 * i));
			i++;
		}

	} // end main()

} // end WhileMain02
