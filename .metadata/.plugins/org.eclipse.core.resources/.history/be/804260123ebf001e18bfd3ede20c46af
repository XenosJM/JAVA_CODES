package edu.java.variable05;

public class CharacterMain {

	public static void main(String[] args) {
		System.out.println("문자(char) 자료형");
		
		char ch1 = 'A';
		System.out.println("ch1 = " + ch1);
		
		char ch2 = 67;
		System.out.println("ch2 = " + ch2);
		
		char ch3 = '1';
		System.out.println("ch3 = " + ch3);
		
	} // end main()

} // end CharacterMain
