package edu.java.while01;

/* while문
 * - for문과 같은 반복문
 * - 선언 형태
 * 		while(조건) {// while : 조건이 참인 동안
 * 			(본문)
 * 		}
 * */


public class WhileMain01 {

	public static void main(String[] args) {
		System.out.println("while 반복문");
		
		// 0 ~ 10 까지 출력
		int x = 0;
		while (x <= 10) {
			System.out.print(x + " ");
			x++;
		}
		
		
		
		
	} // end main()

} // end WhileMain01
