package edu.java.codeup;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();

		if (a <= b) {
			if (a <= c) {
				if (b <= c) {
					System.out.println(a + " " + b + " " + c); // a b c
				} else {
					System.out.println(a + " " + c + " " + b); // a c b
				}
			}
		}

	} // end main()

} // end Main
