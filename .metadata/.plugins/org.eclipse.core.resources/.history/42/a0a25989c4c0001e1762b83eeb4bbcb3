package edu.java.for04;

public class ForMain04 {

	public static void main(String[] args) {
		// 1 ~ 10까지 더하시오.
		
		// 컴퓨터가 계산하는 방식 : 
		// sum1 = 0
		// sum1 = sum1 + 1
		// sum1 = sum1 + 2
		// sum1 = sum1 + 3
		// ...
		
		int sum1 = 0;
		for(int i = 1; i <= 10; i++) {
			sum1 = sum1 + i;
		}
		System.out.println(sum1);
	} // end main()

} // end ForMain04
