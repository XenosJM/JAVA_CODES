package edu.java.codeup;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();

		int first;
		int second;
		int third;

		// ==============================================================================

		// && 사용한 코드
		if (a <= b && a <= c) {
			if (b <= c) {
				first = a;
				second = b;
				third = c;
			} else {
				first = a;
				second = c;
				third = b;
			}
		} else if (b <= c && b <= a) {
			if (a <= c) {
				// b a c
				first = b;
				second = a;
				third = c;
			} else {
				// b c a
				first = b;
				second = c;
				third = a;
			}
		} else if (c <= a && c <= b) {
			if (b <= a) {
				// c b a
				first = c;
				second = b;
				third = a;
			} else {
				// c a b
				first = c;
				second = a;
				third = b;
			}

		}

		System.out.println(first + " " + second + " " + third);

	} // end main()

} // end Main
